package day15;

public class arrayForEach {
	
	public static void main(String[] args) {
		
		String a [] = {"Java", "Hello", "Programming"};
		
		for (String i : a) {
		// i라는 변수가 a라는 배열을 순환할게
		System.out.println(i);
		}
		
		int b[] = {1, 2, 3, 4, 5};
		
		for (int i : b) {
			
			System.out.println(i);
			
		}
	}

}

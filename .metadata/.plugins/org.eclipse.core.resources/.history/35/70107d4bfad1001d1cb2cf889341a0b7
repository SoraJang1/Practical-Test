		int bonus;
		int bonusUser;
		
		int cnt=0;
		
		for (int i=0; i<6; i++) {
			lottoArr [i] = rd.nextInt(45)+1;
		}
		bonus = rd.nextInt(45)+1;
		
		
		
		
		
	}

}
